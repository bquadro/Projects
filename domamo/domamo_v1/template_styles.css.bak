.b_tech_param .param .p_name{float: left; color: #757a80; font-size: 14px; background-color: #fff; padding: 0 5px 0 0;}
.b_tech_param .param .b_value{float: right; font-size: 13px; color: #000; background-color: #fff; padding: 0 0 0 5px; font-family: bold, arial;}
.b_tech_param .param .b_value sup{float: right; font-family: 'IstokRegular', arial;}
.b_tech_param .param .q{background: #fff url(./images/filtr_quest.png) no-repeat 5px 0; float: left; width: 14px; height: 14px; padding: 0 5px 0 5px; margin: 2px 0 0 -5px;}
.b_tech_param .p_more{color: #007ac5; font-size: 14px; text-decoration: none; border-bottom: 1px #007ac5 solid; float: left; line-height: 16px; line-height: 14px; margin: 13px 0 0 0;}
.b_tech_param .p_more:hover{color: #ff1054; border-bottom-color: #ff1054;}
.b_project .b_more_link{float: left; width: 100%; margin: 3px 0; color: #757a80; font-size: 13px; line-height: 16px;}
.b_project .b_more_link b{font-family: bold, arial; color: #000;}
.b_project .b_more_link a{color: #757a80; text-decoration: none;}
.b_project .b_more_link a:hover{color: #ff1054; text-decoration: underline;}

.b_projet_desc .title_link{float: right; color: #007ac5; font-size: 16px; margin: -20px 0 0 0; line-height: 14px; border-bottom: 1px #007ac5 solid; }
.b_projet_desc .title_link:hover{color: #ff1054; border-bottom-color: #ff1054;}

.b_plane_list{float: left; width: 74%; margin: 5px 0 38px 0;}
.b_plane_list .plane{display: block; float: left; min-width: 245px; width: 43%;  margin: 0 40px 20px 0; background-color: #fff; padding: 12px 0 10px 0;}
.b_plane_list .plane img{ width: 100%; display: block; padding: 10px 0 0 0;}
.b_plane_list .plane .b_name{color: #313537; font-size: 18px; border-bottom: 1px #313537 dashed; float: left; line-height: 16px; margin: 0 0 0 15px; clear: both;}
.b_plane_list .plane:hover{box-shadow: 0 0 20px 0 rgba(164, 175, 184, 0.15);}
.b_plane_list .plane:hover .b_name{color: #ff1054; border-bottom-color: #ff1054;}

/*Нет минимального плана*/
.b_plane_list_full{width: 100%; padding-right: 0;}
.b_plane_list_full .plane{width: 31%; margin-right: 2%!important; min-width: 252px}

.b_projet_desc .plane_min{display: block; float: right; min-width: 163px; width: 24%; margin: 5px 0 10px 0; background-color: #fff; padding: 12px 0 12px 12px;}
.b_projet_desc .plane_min img{max-width: 100%;}
.b_projet_desc .plane_min .b_name{color: #313537; font-size: 18px; border-bottom: 1px transparent dashed; float: right; line-height: 16px; width: 100%;}
.b_projet_desc a.plane_min:hover{box-shadow: 0 0 20px 0 rgba(164, 175, 184, 0.15);}
.b_projet_desc a.plane_min:hover .b_name{color: #ff1054; border-bottom-color: transparent;}
.b_projet_desc .plane_min .b_param{float: left; width: 102px; margin: 2px 0; clear: both;}
.b_projet_desc .plane_min .b_param .p_name{float: left; color: #757a80; font-size: 14px; background-color: #fff; padding: 0 5px 0 0;}
.b_projet_desc .plane_min .b_param .p_val{float: right; font-size: 14px; color: #000; background-color: #fff; padding: 0 0 0 5px;}
.b_projet_desc .plane_min .b_param .p_val sup{float: right;}

.b_fasads{float: left; width: 100%; margin: 0 0 9px 0;}
.b_fasads .b_fasad{float: left; width: 22%; margin-right: 3%;  margin-bottom: 20px;}
.b_fasads .b_fasad img{float: left; padding: 5px 3%; background-color: #fff; float: left; width: 94%;}
.b_fasads .b_fasad:hover{box-shadow: 0 0 20px 0 rgba(164, 175, 184, 0.15);}
.b_fasads .b_fasad.last{margin-right: 0; float: right;}
.b_projet_desc .b_full_desc{font-size: 13px; color: #222426; float: left; width: 100%; line-height: 18px; /*margin: -6px 0 14px 0; */ margin: 0 0 14px 0;}
.b_projet_desc .b_full_desc p{margin: 0 0 10px;}

/*Variant lots list*/
.b_projet_desc .projects .wrap_item {height: auto; overflow: visible;}
.b_projet_desc .projects .wrap_item2 {width: 100%; height: auto; overflow: visible; padding: 0;}
.b_project_big_text{ background-color: #fff; box-shadow:  0 0 25px 4px rgba(193, 192, 189, 0.25); padding: 20px 60px; margin: 20px 0; font-size: 14px;}


.file_list{float: left; width: 100%;}
.b_file{width: 270px; margin: 14px 30px 5px 3px; padding: 0 0 0 51px; display: block; float: left; position: relative; min-height: 44px;}
.b_file .ico{ width: 33px; height: 44px; background: #d01c28; margin: 0 0 0 0; display: block; padding: 0; color: #fff; font-size: 12px; text-align: center; line-height: 48px; position: absolute; left: 0; top: 0;}
.b_file:hover .ico{color: #fff;}
.b_file .ico2{border: 6px #ff2e5a solid; border-right-color: #fbfbfb; border-top-color: #fbfbfb; position: absolute; top: 0; right: 0;}
.b_file .b_name{color: #007ac5; font-size: 14px; display: block; text-decoration: none; margin: 2px 0 3px; border-bottom: 1px #007ac5 solid; float: left; line-height: 18px;}
.b_file:hover .b_name{color: #ff1054; border-bottom-color: #ff1054;}
.b_file .b_param{color: #a7a7a7; display: block; font-size: 12px;}
/*color ico*/
.b_file.ico_blue .ico{background-color: #278ee5;}
.b_file.ico_blue .ico2{border-bottom-color: #16baf0; border-left-color: #16baf0;}
.b_file.ico_grey .ico{background-color: #c3cdd5;}
.b_file.ico_grey .ico2{border-bottom-color: #607485; border-left-color: #607485;}
.b_file.ico_size_4 .ico{font-size: 10px;}



.btn_140x40{background: url(images/filtr_btn.png) no-repeat left top; width: 141px; height: 41px; display: block; color: #fff!important; font-size: 13px; font-family: bold, arial; text-align: center; line-height: 42px; text-transform: uppercase; text-shadow: 0 0 3px rgba(0,0,0, 0.1); border: 0px none; cursor: pointer;}
.btn_140x40:hover{background-position: 0 -41px;}
.btn_140x40:active{background-position: 0 -82px;}


.b_fancy_result{padding: 26px 50px; width: 300px;}
.b_form form{padding: 26px 50px 20px; max-width: 556px;}
.b_form .b_title{font-size: 22px; color: #313537; font-weight: 900;}

.b_form .b_block{float: left; width: 290px; margin: 0}
.b_form .b_block.last{width: 265px;}
.b_form .b_block_full{width: 558px; margin: 0 0 10px 0;}
.b_form .b_input{display: block; float: left; width: 264px; height: 26px; background: url(./images/ui/form_input1.png) no-repeat 0 0; padding: 10px; margin: -8px 0 0 -10px;}
.b_form .b_input.b_active{background-position:  0 -46px;}
.b_form .b_input.b_error{background-position: 0 -92px;}
.b_form .b_input input{display: block; background-color: transparent; width: 244px; height: 25px; border: 0px none; padding: 0 10px; line-height: 26px; font-family: arial; font-size: 14px; color: #000;}

.b_form .b_textarea{display: block; float: left; width: 557px;; height: 115px; background: url(./images/ui/form_textarea.png) no-repeat 0 0; padding: 10px; }
.b_form .b_textarea.b_active{background-position: 0 -135px;}
.b_form .b_textarea.b_error{background-position: 0 -270px;}
.b_form .b_textarea textarea{display: block; background-color: transparent; width: 537px; height: 105px; border: 0px none; padding: 5px 10px; line-height: 16px; font-family: arial; font-size: 14px; color: #000; resize: none;}
.b_form label {color: #979797; font-size: 14px;}
.b_form .req{color: #009b83;}
.b_form .b_req{display: block; background-color: #eee; padding: 10px 50px; font-size: 12px; color: #009b83;}

