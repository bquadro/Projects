.b_dop_content, .b_tech_content{max-width: 1950px; margin: auto; padding: 30px 20px; min-height: 500px;}
.b_dop_content{max-width: 1500px;}
.b_dop_content .b_content_2{  background-color: #fff; box-shadow: 0 0 25px 4px rgba(193, 192, 189, 0.25); padding: 20px 60px; margin: 20px 0; font-size: 14px;}

.title_page{margin-top: 0; padding-top: 0;}
.b_content ul {margin: 10px 0 10px 0px; padding: 0 0 0 30px; overflow: hidden;}
.b_content ul li{background: url(images/list_marker.png) no-repeat left 5px; list-style: none; margin: 5px 0; padding: 0 0 0 20px;}

/*UI*/
.checkbox{ width: 22px; height: 22px; cursor: pointer; background: url(./images/ui/checkbox.png) no-repeat center top; display: block;}
.checkbox:hover{background-position: 0 -22px;}
.checkbox.active{background-position: 0 -44px;}
.checkbox.active:hover{background-position: 0 -66px;}


.b_header {background: #1b1e26;}
.b_header a{text-decoration: none;}
.top_header {width: 100%; background: #101010; color: #fff; /*padding: 20px 0 15px; */; padding: 11px 0 11px; line-height: 24px;}
.top_header .city_select{float: left; padding: 12px 10px 12px 20px; min-width: 177px; margin: -12px 0 -12px 0;}
.top_header .city_select .city {color: #fff; float: left;}
.top_header .city_select .city_active{float: left; position: relative; padding: 0 0 0 19px; cursor: pointer;} 
.top_header .city_select .city_active:before{position: absolute; display: block; content: ''; left: 6px; top: 9px; border: 4px solid transparent; border-top: 4px solid #FFF;}
.top_header .city_select .city_active:hover{color: #ff1054;}
.top_header .city_select .city_active:hover:before{color: #ff1054; border-top-color: #ff1054;}
.top_header .city_select.active .city_active:hover{color: #fff;}
.top_header .city_select.active .city_active:hover:before{color: #fff; border-top-color: #fff;}

.top_header .city_select.active{background-color: #1b1e26; position: relative;}
.top_header .city_select.active .city_active{opacity: 0.2;}
.top_header .city_select.active > ul{display: block; }
.top_header .top_header_ul {display: none; position: absolute; background: #1b1e26; left: 0; right: 0; top: 30px; z-index: 999 !important; font-size: 15px; padding: 15px; margin: 0; }
.top_header .top_header_ul li {display: block; list-style: none; color: #FFF; height: 28px; line-height: 28px; padding-left: 15px; cursor: pointer;}
.top_header .top_header_ul li:hover{color: #ff1054;}
.top_header .top_header_ul li.active{color: #FFF; background: #00afef; -webkit-border-radius: 3px; -moz-border-radius: 3px; border-radius: 3px; margin: 3px 0;}


.top_header .tel {float: left; width: 138px; font-size: 15px;}
.top_header .connect {color: #7aceff; text-decoration: underline; float: left; font-size: 15px; }
.top_header .connect:hover{color: #ff1054; }
.social {float: left; height: 25px; margin: -1px 0 0 32px; }
.social a {display: block; width: 25px; height: 25px; float: left; background: url('./images/index/ico/ico.png') no-repeat left center; margin: 0 6px 0 0;}
.social a:hover{opacity: 0.9;}
.social a.twitter{background-position: left center;}
.social a.facebook{background-position: -32px center;}
.social a.youtube{background-position: -64px center;}
.social a.vk{background-position: right center;}

.top_header .b_wrapper{position: relative;}
.top_header .b_search {float: left; position: absolute; left: 300px; right: 200px; left: 670px; right: 440px; margin: -1px 0 0 0; z-index: 1; background: #101010; max-width: 550px;}
.top_header .b_search{
	transition: left 300ms ease-in-out, right 300ms ease-in-out, max-width 300ms ease-in-out;
  -moz-transition: left 300ms ease-in-out, right 300ms ease-in-out, max-width 300ms ease-in-out;
  -webkit-transition: left 300ms ease-in-out, right 300ms ease-in-out, max-width 300ms ease-in-out;
  -o-transition: left 300ms ease-in-out, right 300ms ease-in-out, max-width 300ms ease-in-out;
}
.top_header .b_search input[type="submit"] {width: 15px; height: 16px; position: absolute; top: 4px; right: 10px; background: url('./images/index/ico/search.png') no-repeat center top; border: 0px; cursor: pointer; z-index: 6;}
.top_header .b_search input[type="submit"]:hover{background-position: center -16px;}

.top_header .b_search .title{color: #b6babd; display: none; position: absolute; left: 15px; top: 3px; z-index: 6; font-size: 14px; opacity: 0.5;}
.top_header .b_search input.search_text{width: 87%;  height: 24px; padding: 0 10% 0 3%; border-radius: 30px; background: transparent; border: 1px solid #3f4345; color: #FFF; font-family: 'IstokRegular'; font-size: 14px; z-index: 5; position: relative; line-height: 28px;}
.top_header .b_search .placeholder {color: #3f4345; font-family: 'IstokRegular';  position: absolute; left: 3%; top: 2px; z-index: 1;}
.top_header .b_search.query .placeholder{display: none;}
.top_header .b_search.focus{left: 10px; right: 10px; margin: -2px 0 0 0; max-width: 100%;}
.top_header .b_search.focus .title{display: block;}
.top_header .b_search.focus input.search_text{padding: 0 10% 0 4%; width: 86%; height: 26px; background-color: #101010; line-height: 30px;}
.top_header .b_search input[type="submit"]{top: 5px;}

.top_header .b_right{float: right; margin: 0 11px 0 0;}
.top_header .profil,
.top_header .favorite,
.top_header .compare {float: left; color: #7aceff; text-decoration: none; line-height: 17px; margin: 1px 0 0 36px; font-size: 15px; border-bottom: 1px #7aceff solid;}
.top_header .profil:hover,
.top_header .favorite:hover
/*.top_header .compare:hover*/ { color: #ff1054; border-bottom-color: #ff1054;}
.top_header .favorite {right: 145px; padding-left: 22px; background: url('./images/index/ico/heart.png') no-repeat left 3px; border: none;}
.top_header .favorite:hover {background-position: left -20px; border: none;}
.top_header .favorite span{border-bottom: #7aceff 1px solid; display: block; float: left;}
.top_header .favorite:hover span{border-bottom: #ff1054 1px solid; }
.top_header .compare {color: #c9d1d5; border-bottom-color: #c9d1d5;}
.top_header .compare.active {color: #7aceff; border-bottom-color: #7aceff;} 
.top_header .compare.active:hover {color: #ff1054; border-bottom-color: #ff1054;} 

.top_header .compare:hover{color: #FFF; border-bottom-color: #fff;}
.header_two {width: 100%;height: 54px; background: #1b1e26; font-size: 15px; }
.header_two .logo {display: block; height: 25px; float: left; margin: 15px 51px 0 20px;}
@media only screen and (-webkit-min-device-pixel-ratio: 2) {
		.header_two .logo{
				display: block;
				background:  url('/images/@logo.png') no-repeat;
				background-size: 100%;
		}	
   .header_two .logo img{				
			opacity: 0;
   }
}
.header_two .main_menu_ul {float: left; margin: 0; padding: 0;}
.header_two .main_menu_ul > .main_menu_li {float: left; padding: 0; list-style: none;}
.header_two .main_menu_ul > .main_menu_li > a {display: block; line-height: 58px; text-transform: uppercase; color: #FFF; padding: 0 10px; height: 54px; letter-spacing: 0.5px;}
.header_two .main_menu_ul > .main_menu_li > a.active{background-color: #00afef;}
.header_two .main_menu_ul > .main_menu_li:hover > a.active{background: #2a2f3d;}
.header_two .small_cart {display: block; width: 136px; height: 54px; line-height: 58px; float: right; text-transform: uppercase; color: #FFF; padding: 0 15px 0 30px; position: relative; margin: 0 0 0 0; text-decoration: none;}
.header_two .small_cart:hover{background: #2a2f3d;}
.header_two .small_cart span { display: block; position: absolute; text-align: center; width: 20px; height: 20px; top: 11px; right: 12px;	font: 14px bold; line-height: 20px;
																						-webkit-border-radius: 50%; -moz-border-radius: 50%; border-radius: 50%; background: #ffb302; background: -webkit-gradient(linear, left top, left bottom, from(#ffb302), to(#f08b00)); background: -webkit-linear-gradient(top, #ffb302, #f08b00); background: -moz-linear-gradient(top, #ffb302, #f08b00); background: -o-linear-gradient(top, #ffb302, #f08b00); background: linear-gradient(top, #ffb302, #f08b00); }
.header_two .main_menu_ul li{list-style: none;}
.main_menu_li:hover,
.main_menu_li .mm_li_ul { background: #2a2f3d; }
.main_menu_li:hover .mm_li_ul {display: block !important; }

/*need test*/
.main_menu_li  .mm_li_ul { display: none; position: absolute; line-height: 20px; font-size: 14px; z-index: 999; width: 1045px; padding: 20px 0;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item { display: inline-block; margin-right: -5px; width: 24%; vertical-align: top;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item:nth-child(3) { width: 20%}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item:last-child { margin-right: 0; width: 28%;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item span { display: block; padding: 0 20px;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item span.bold { text-transform: uppercase; font-size: 13px; font-weight: 900; color: #88a1b6; margin-bottom: 11px;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item a { display: block; padding: 0 20px; color: #FFF; height: 28px; line-height: 28px;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item a:hover { text-decoration: underline}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item a.new { color: #00afef}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item a.new_first { margin-top: 25px}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item a.active { background: #00afef; -webkit-border-radius: 4px; -moz-border-radius: 4px; border-radius: 4px;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item a.active:hover { text-decoration: none}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item a sup { font-size: 10px; position: relative; left: 3px;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item .b_left, .main_menu_li  .mm_li_ul .mm_li_ul_li .item .b_right { display: inline-block; margin-right: -5px; width: 69%; vertical-align: top;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item .b_left .b_bold, .main_menu_li  .mm_li_ul .mm_li_ul_li .item .b_right .b_bold { font-size: 14px; color: #88a1b6; display: block; padding: 0 20px; height: 28px; line-height: 28px;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item .b_right { margin-right: 0; width: 30%; text-align: center;}
.main_menu_li  .mm_li_ul .mm_li_ul_li .item .b_right .b_bold { padding: 0; text-align: center !important;}

.main_slider {position: relative; background: #1b1e26; }
.main_slider .loading{background: url(./images/loading.gif) no-repeat; width: 128px; height: 128px; margin: auto; position: absolute; top: 0; left: 0; right: 0; bottom: 0; z-index: 2;}
.main_slider > div{margin: 0 auto; padding: 0; position: relative; display: block; width: 100%;}
.main_slider ul{margin: 0 auto; padding: 0; position: relative; display: block; width: 100%;}
.main_slider ul li{ position: relative; height: 100%; width: 100%; overflow: hidden; margin: 0; padding: 0; background: transparent;}
.main_slider ul li.active{display: block;}
.main_slider ul li .b_image{display: block; position: absolute; margin: auto; top: 0; bottom: 0; left: 0; right: 0;}
.main_slider ul li .b_image img{display: block;  }

.main_slider .text{position:absolute; width:440px; left: 22px; bottom: 35px; color: #fff; }
.main_slider .cycle-slide-active .text{display: block;}
.main_slider .text .description{display:block; font-size: 24px; line-height:28px; margin-bottom:20px;  text-shadow: 0 0 40px #333; }
.main_slider .text > a{color: #fff;}
.main_slider .text .number,
.main_slider .text .area{background:#00afef; height:35px; font-family: bold, arial; padding:0 10px; float: left; line-height: 40px; font-size: 20px;}
.main_slider .text .area{background:#292e44; float: left;}
.main_slider .text .number{text-transform: uppercase;}
.main_slider .number:hover{background-color: #1579d7;}

.main_slider .pager{position: absolute; bottom: 20px; height: 20px; left: 50%; z-index: 300;}
.main_slider .pager > div{margin: 0 0 0 -50%; float: left; }
.main_slider .pager span{background-color: #121515; float: left; display: block; width: 12px; height: 12px; margin: 0 4px; border-radius: 6px; cursor: pointer; text-indent: -9000px;}
.main_slider .pager span.cycle-pager-active{background-color: #00afef;}

.arrow_right, .arrow_left{background: url(./js/fotorama/fotorama_arrow.png) no-repeat 0 0;   width: 32px; height: 48px; position: absolute; left: 22px; top: 50%; margin-top: -24px; display: block; z-index: 300; cursor: pointer;}
.arrow_right{background-position: 100% 0; right: 22px; left: auto;}
.arrow_right:hover{background-position: 100% 100%;}
.arrow_left:hover{background-position: 0 100%;}

.front_filter  { position: absolute; width: 1014px; padding: 16px 0; background: rgba(0, 0, 0, 0.7); z-index: 99; top: 50%; left: 50%; margin-top: -38px; margin-left: -507px; padding-left: 15px; z-index: 300;} 
.front_filter .slogan{position: absolute; top: -75px; font-size: 52px; color: #fff; text-align: center; width: 100%; text-shadow: 0 0 40px #333;}
.front_filter input  { float: left; border: 0px; display: block; height: 43px;} 
.front_filter .block{position: relative; float: left; height: 41px;}
.front_filter .block .b_title{width: 96px; height: 41px; line-height: 44px; border: 1px #fff solid; margin-right: 6px; background: #fff url('./images/index/filter/ico.png') no-repeat 2px 4px; float: left; color: #808080; padding: 0 0 0 50px; cursor: pointer; position: relative;}
.front_filter .block .b_title:hover{border-color: #00afef; box-shadow: 0 0px 14px 0px #24afff; }

.front_filter .block2 .b_title{ background-position: 2px -45px } 
.front_filter .block3 .b_title{ background-position: 2px -112px } 
.front_filter .block4 .b_title{ background-position: 2px -162px } 
.front_filter .block5 .b_title{background: #fff; padding: 0; position: relative; width: 205px; overflow: hidden;}
.front_filter .block5 input{ width: 175px; height: 43px; line-height: 43px; margin-right: 5px; padding: 0 15px !important;  background-color: transparent; z-index: 10; position: relative; font-family: 'IstokRegular', arial; } 
.front_filter .block5 input:focus, .front_filter .block5 input.active{background-color: #fff;}
.front_filter .block5 .placeholder{ left: 15px; color: #808080; font-size: 16px; position: absolute; z-index: 5;} 
.front_filter .block5 .active .placeholder{display: none;}
.front_filter .plus  { display: block; margin-right: 4px; width: 46px; height: 41px; background: url('./images/index/filter/plus.png') no-repeat center top; float: left; border: 1px #fff solid;} 
.front_filter .plus:hover{border-color: #00afef; box-shadow: 0 0px 14px 0px #24afff; }
.front_filter input[type="submit"]  { top: 14px; width: 118px; height: 45px; background: url('./images/index/filter/submit.png') no-repeat center top; cursor: pointer;} 
.front_filter input[type="submit"]:hover  { background: url('./images/index/filter/submit.png') no-repeat center -46px } 
.front_filter input[type="submit"]:active  { background: url('./images/index/filter/submit.png') no-repeat center -92px }



.front_filter .block .b_hide{display: block; position: absolute; top: 58px; left: 0; background-color: #fff; box-shadow: 0 0 15px 0 #333;}
.front_filter .block.b_hide:before{}
.front_filter .b_ok {  height: 20px; background: #fff; cursor: pointer; float: right; color: #000; font-size: 12px; font-family: 'IstokRegular'; padding: 0 12px; border-radius: 2px; margin: 9px 0 0 0; line-height: 22px;} 
.front_filter .b_ok{ -webkit-box-shadow: 0 0px 8px 1px rgba(0, 0, 0, 0.2); -moz-box-shadow: 0 0px 8px 1px rgba(0, 0, 0, 0.2); box-shadow: 0 0px 8px 1px rgba(0, 0, 0, 0.2); text-transform: uppercase;}
.front_filter .b_ok:hover { background: #00afef; color: #FFF;}
.front_filter .b_ok:active { background: #4875a8; color: #FFF;} 

.front_filter .block.active:after{content: ""; position: absolute; border: transparent 7px solid; border-top-color: #fff; top: 42px; left: 50%; margin-left: -7px;}
.front_filter .block .b_hide{font-size: 16px; line-height: 20px; display: none;}


.front_filter .b_scroller{padding: 25px 20px 24px; width: 480px;}
.front_filter .b_scroller .b_size{float: left; margin: 0 5px 0 20px; font-size: 32px; font-family: bold, arial; color: #000;}
.front_filter .b_scroller .ms{float: left; font-size: 24px; color: #000; font-family: bold, arial; line-height: 26px; }
.front_filter .ui-slider{background: url(./images/ui/scroll_line.png) repeat-x 0 -1px; width: 300px; height: 12px; position: relative; border-radius: 6px; padding: 0; float: left; margin: 4px 0 0 0; box-shadow: 0 0 0 1px #b3b3b3; }
.front_filter .ui-widget-header{background: url(./images/ui/scroll_line.png) repeat-x 0 -14px; height: 14px; position: relative; border-radius: 6px; margin: -1px 0 0 0;}
.front_filter .ui-slider-handle{width: 28px; height: 42px; margin: 0; display: block; position: absolute; background: url(./images/ui/scroll_btn.png) no-repeat 0 0; top: -12px; margin-left: -14px;}
.front_filter .ui-state-hover, .front_filter .ui-state-active{background-position: 0 -42px;}

.front_filter .b_room{float: left; height: 75px; width: 443px; overflow: hidden;}
