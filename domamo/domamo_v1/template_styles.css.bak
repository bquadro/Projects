.front_filter .b_remove{position: absolute; right: 6px; top: 3px; width: 42px; height: 39px; background: url(./images/front_filtr_clear.png) no-repeat 0 0; display: none; cursor: pointer;}
.front_filter .change .b_remove{display: block; z-index: 100;}
.front_filter .change .b_title{color: #000; white-space: nowrap;}
.front_filter .b_remove:hover{background-position: 100% 100%;}

